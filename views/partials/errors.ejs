<% if (err) { %>
    <% err.forEach((result)=>{ %>
        <div class="alert alert-danger" role="alert">    
            <%= result.message %>
        </div>
    <% }) %>
<% } %>
<% if (msg) { %>
    <div class="alert alert-danger" role="alert">
        <%= msg %>
    </div>
<% } %>

<% if (success_msg!= '') { %>
    <div class="alert alert-success" role="alert">
        <%= success_msg %>
    </div>
<% } %>

<% if (error_msg!= '') { %>
    <div class="alert alert-danger" role="alert">
        <%= error_msg %>
    </div>
<% } %>

<% if (error!= '') { %>
    <div class="alert alert-danger" role="alert">
        <%= error %>
    </div>
<% } %>